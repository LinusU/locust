{
  "name": "@buttercup/locust",
  "version": "0.0.0",
  "description": "Locust Login Toolkit",
  "main": "dist/locust.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "dev": "webpack -w --progress",
    "format": "prettier --write '{{source,test}/**/*.js,webpack.config.js}'",
    "precommit": "lint-staged",
    "prepublishOnly": "run-s build",
    "pub": "np",
    "test": "run-s test:format",
    "test:format": "prettier-check '{{source,test}/**/*.js,webpack.config.js}'"
  },
  "lint-staged": {
    "{{source,test}/**/*.js,webpack.config.js}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eventemitter3": "^3.0.0",
    "husky": "^0.14.3",
    "is-visible": "^2.1.2",
    "lint-staged": "^6.0.0",
    "np": "^2.18.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.2",
    "prettier-check": "^2.0.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0"
  }
}
